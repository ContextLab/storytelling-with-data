# convert year ranges to average years

def str2year(s):
  s = str(s)
  if '-' in s:
    ind = s.find('-')
    return np.mean([int(s[:ind]), int(s[(ind + 1):])])
  elif ',' in s:
    return np.mean(eval('[' + s + ']'))
  else:
    return float(s)

access['year'] = access['Year(s)'].apply(str2year)

